{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "#/*": ["*"]
    },
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./dist" ,
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true ,
    "strict": true ,
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  }
}